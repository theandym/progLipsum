<h1>New dataset</h1>

<form id="new-dataset" name="dataset">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value="<%= name %>" >
  </div>

  <div class="field">
    <label for="format"> format:</label>
    <input type="text" name="format" id="format" value="<%= format %>" >
  </div>

  <div class="field">
    <label for="output"> output:</label>
    <input type="text" name="output" id="output" value="<%= output %>" >
  </div>

  <div class="field">
    <label for="public"> public:</label>
    <input type="text" name="public" id="public" value="<%= public %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Dataset" />
  </div>

</form>

